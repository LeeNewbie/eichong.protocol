<?xml version="1.0" encoding="UTF-8" ?> 
<configs>
	<CommonRealFields>
		<YC_WORKSTATUS>
			<type>3</type>
			<addr>1</addr>
		</YC_WORKSTATUS>
		<YX_1_LINKED_CAR>
			<type>1</type>
			<addr>1</addr>
		</YX_1_LINKED_CAR>
		<YX_1_CARD_READER_FAULT>
			<type>1</type>
			<addr>6</addr>
		</YX_1_CARD_READER_FAULT>
		<YX_2_CHARGE_OVER_TEMP>
			<type>2</type>
			<addr>2</addr>
		</YX_2_CHARGE_OVER_TEMP>
		<YX_2_OUT_LINKER_STATUS>
			<type>2</type>
			<addr>13</addr>
		</YX_2_OUT_LINKER_STATUS>
		<YC_A_VOL>
			<type>3</type>
			<addr>41</addr>
		</YC_A_VOL>
		<YC_B_VOL>
			<type>3</type>
			<addr>42</addr>
		</YC_B_VOL>
		<YC_C_VOL>
			<type>3</type>
			<addr>43</addr>
		</YC_C_VOL>
		<YC_A_CURRENT>
			<type>3</type>
			<addr>44</addr>
		</YC_A_CURRENT>
		<YC_B_CURRENT>
			<type>3</type>
			<addr>45</addr>
		</YC_B_CURRENT>
		<YC_C_CURRENT>
			<type>3</type>
			<addr>46</addr>
		</YC_C_CURRENT>
		<YC_OUT_VOL>
			<type>3</type>
			<addr>3</addr>
		</YC_OUT_VOL>
		<YC_OUT_CURRENT>
			<type>3</type>
			<addr>4</addr>
		</YC_OUT_CURRENT>
		<YC_VAR_ACTIVE_TOTAL_METERNUM>
			<type>4</type>
			<addr>1</addr>
		</YC_VAR_ACTIVE_TOTAL_METERNUM>
		<YC_SOC>
			<type>3</type>
			<addr>5</addr>
		</YC_SOC>
		<YC_REMAIN_TIME>
			<type>3</type>
			<addr>7</addr>
		</YC_REMAIN_TIME>
	</CommonRealFields>

	<StopCarOrgan>
		<sendConfig>
			<Valid>1</Valid>
			<SendCycle>60</SendCycle>
		</sendConfig>
		<Fields>
			<chargepoleStatus>
				<dataformat>varchar2(8)</dataformat>
				<value>if(${YC_WORKSTATUS}=0,4,if(${YC_WORKSTATUS}=3,3,if(${YC_WORKSTATUS}=1 || ${YC_WORKSTATUS}>30,1,2))) </value>
			</chargepoleStatus>
			<gunConnStatus>
				<dataformat>varchar2(8)</dataformat>
				<value>${YX_1_LINKED_CAR}</value>
			</gunConnStatus>
			<faultCode>
				<dataformat>varchar2(8)</dataformat>
				<value>if(${YC_WORKSTATUS}=38,0,if(${YC_WORKSTATUS}=35,1,if(${YC_WORKSTATUS}=36,2,if(${YC_WORKSTATUS}=37,6,if(${YX_1_CARD_READER_FAULT}=1,3,if(${YX_2_CHARGE_OVER_TEMP}=1,4,if(@curType=5 and ${YX_2_OUT_LINKER_STATUS}=1,5,7))))))) </value>
			</faultCode>
			<voltA>
				<dataformat>number(16,6)</dataformat>
				<value>${YC_A_VOL}*0.1</value>
			</voltA>
			<voltB>
				<dataformat>number(16,6)</dataformat>
				<value>${YC_B_VOL}*0.1</value>
			</voltB>
			<voltC>
				<dataformat>number(16,6)</dataformat>
				<value>${YC_C_VOL}*0.1</value>
			</voltC>
			<curA>
				<dataformat>number(16,6)</dataformat>
				<value>${YC_A_CURRENT}*0.01</value>
			</curA>
			<curB>
				<dataformat>number(16,6)</dataformat>
				<value>${YC_B_CURRENT}*0.01</value>
			</curB>
			<curC>
				<dataformat>number(16,6)</dataformat>
				<value>${YC_C_CURRENT}*0.01</value>
			</curC>
			<power>
				<dataformat>number(16,6)</dataformat>
				<value>0</value>
			</power>
			<volt>
				<dataformat>number(16,6)</dataformat>
				<value>${YC_OUT_VOL}*0.1</value>
			</volt>
			<cur>
				<dataformat>number(16,6)</dataformat>
				<value>${YC_OUT_CURRENT}*0.1</value>
			</cur>
			<edPq>
				<dataformat>number(12,4)</dataformat>
				<value>${YC_VAR_ACTIVE_TOTAL_METERNUM}*0.001</value>
			</edPq>
			<soc>
				<dataformat>number(16)</dataformat>
				<value>if(@curType=5,${YC_SOC},0)</value>
			</soc>
			<restTime>
				<dataformat>number(5)</dataformat>
				<value>${YC_REMAIN_TIME}</value>
			</restTime>
		</Fields>
	</StopCarOrgan>

</configs>